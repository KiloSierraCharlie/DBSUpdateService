{
    "version": "1.1",
    "name": "kilosierracharlie/disclosurebarringservice",
    "description": "A PHP library for interacting with the DBS update service API.",
    "keywords": ["dbs", "disclosure", "crb", "update service", "api"],
    "minimum-stability": "alpha",
    "homepage": "https://github.com/KiloSierraCharlie/DisclosureBarringService",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Kieran Cross",
            "homepage": "https://github.com/KiloSierraCharlie"
        }
    ],
    "autoload": {
        "psr-4": {"KiloSierraCharlie\\DisclosureBarringService\\": "src/"},
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "KiloSierraCharlie\\DisclosureBarringService\\Tests\\": "tests/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^7.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^12"
    }
}
